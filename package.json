{
	"name": "qr-reader",
	"description": "a qr-code scanner that uses a web worker running the zbar web assembly",
	"version": "3.1.0",
	"main": "dist/qr-reader.js",
	"module": "./index.js",
	"scripts": {
		"start": "webpack-dev-server --hot --inline --open --mode=development --host 0.0.0.0 --disable-host-check",
		"build": "cross-env NODE_ENV=production webpack-cli --mode=production --config webpack.config.js",
		"precommit": "lint-staged",
		"pretty": "prettier --write 'app/**/*.js'"
	},
	"lint-staged": {
		"*.{js,css}": [
			"npm run pretty",
			"git add"
		]
	},
	"engines": {
		"node": ">=4.1.1"
	},
	"dependencies": {
		"is-url": "^1.2.4",
		"webpack-loader": "^0.0.1",
		"worker-loader": "^2.0.0"
	},
	"devDependencies": {
		"clean-webpack-plugin": "^0.1.19",
		"copy-webpack-plugin": "^4.5.3",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"extract-text-webpack-plugin": "4.0.0-beta.0",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"husky": "^1.1.2",
		"lint-staged": "^7.3.0",
		"mini-css-extract-plugin": "^0.4.4",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"prettier": "^1.14.3",
		"style-loader": "^0.23.1",
		"webpack": "^4.29.0",
		"webpack-cli": "^3.1.2",
		"webpack-dev-server": "^3.1.9",
		"workbox-webpack-plugin": "^3.6.2"
	}
}
